            	if(!change){
            		$('#save_notes').addClass('disabled');
        		}else{
            		$('#save_notes').removeClass('disabled');
        		}
            }
	        if(change){
	            $('#changes_pending').css('display','block');
	        }
	        else{
	            $('#changes_pending').css('display','none');
	        }
	    }
    	
